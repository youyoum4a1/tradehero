<resources
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

  <!--
      Base application theme, dependent on API level. This theme is replaced
      by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
  -->
  <style name="TradeHero" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowActionBarOverlay">false</item>
    <item name="colorPrimaryDark">@color/black</item>
  </style>

  <style name="TradeHero.Settings" parent="TradeHero">
    <item name="android:actionBarStyle">@style/TradeHero.Dashboard.ActionBar</item>
    <item name="android:windowActionBarOverlay">false</item>
  </style>

  <style name="AppBaseTheme.NoActionBar" parent="TradeHero">
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:background">@color/splash_background_solid</item>
  </style>

  <style name="AppBaseTheme.NoActionBarTrans" parent="TradeHero">
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:background">@color/transparent</item>
  </style>

  <style name="AppBaseTheme.NoActionBar2" parent="TradeHero">
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowBackground">@color/color_blue</item>
  </style>

  <style name="TradeHero.Menu.ItemTextIcon" parent="Base.TextAppearance.AppCompat.SearchResult">
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">@dimen/text_xlarge</item>
  </style>

  <style name="TH"/>

  <style name="TH.Title">
    <item name="android:textColor">@color/title</item>
    <item name="android:padding">2dp</item>
  </style>

  <style name="TH.SmallTitle" parent="TH.Title">
    <item name="android:textSize">@dimen/text_small</item>
  </style>

  <style name="TH.Value">
    <item name="android:textColor">@color/black</item>
    <item name="android:padding">2dp</item>
  </style>

  <style name="TH.SmallValue" parent="TH.Value">
    <item name="android:textSize">@dimen/text_small</item>
  </style>

  <style name="TH.Btn_Price_Bar_Base">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">match_parent</item>
    <item name="android:layout_weight">1</item>
    <item name="android:gravity">center</item>
    <item name="android:textStyle">bold</item>
  </style>

  <style name="TH.Btn_Price_Bar" parent="TH.Btn_Price_Bar_Base">
    <item name="android:background">@drawable/basic_transparent_selector</item>
    <item name="android:textColor">@color/white</item>
    <item name="android:textSize">@dimen/text_xlarge</item>
  </style>

  <style name="TH.Btn_Time_Span" parent="TH.Btn_Price_Bar_Base">
    <item name="android:background">@drawable/basic_transparent_selector</item>
    <item name="android:textColor">@color/price_bar_text_default</item>
    <item name="android:textSize">@dimen/text_xsmall</item>
    <item name="android:minHeight">36dp</item>
  </style>

  <style name="TH.BtnQuickPriceSet" parent="TH.Btn_Price_Bar_Base">
    <item name="android:background">@drawable/basic_transparent_selector</item>
    <item name="android:textColor">@color/price_bar_text_default</item>
    <item name="android:textSize">@dimen/text_xsmall</item>

  </style>

  <style name="TH.Btn_Price_Bar_Separator">
    <item name="android:layout_width">1dp</item>
    <item name="android:layout_height">match_parent</item>
    <item name="android:background">@color/price_bar_text_default</item>
  </style>

  <style name="TradeHero.Authentication"
      parent="TradeHero">
    <item name="android:editTextStyle">@style/TradeHero.Authentication.EditText</item>
    <item name="android:paddingTop">@dimen/common_margin_80</item>
    <item name="android:paddingRight">@dimen/common_margin_30</item>
    <item name="android:paddingLeft">@dimen/common_margin_30</item>
    <item name="android:paddingBottom">@dimen/common_margin_10</item>
  </style>

  <style name="AppBaseTheme.Welcome" parent="TradeHero">
    <item name="android:paddingTop">65dip</item>
    <item name="android:paddingLeft">30dip</item>
    <item name="android:paddingRight">30dip</item>
    <item name="android:background">@color/authentication_background_color</item>
  </style>

  <style name="AppBaseTheme.Welcome.SignIn" parent="@android:style/Widget.Button">
    <item name="android:layout_marginTop">0dip</item>
  </style>

  <style name="TradeHero.ButtonTextShadow">
    <item name="android:shadowRadius">5.6</item>
    <item name="android:shadowDx">0</item>
    <item name="android:shadowDy">-1</item>
    <item name="android:shadowColor">#000000</item>
  </style>

  <style name="TradeHero.Dashboard" parent="TradeHero">
    <!--  <item name="android:homeAsUpIndicator">@drawable/button_back</item>  -->
    <item name="android:windowActionBarOverlay">false</item>
    <item name="android:actionBarStyle">@style/TradeHero.Dashboard.ActionBar</item>
    <item name="android:actionButtonStyle">@style/TradeHero.Dashboard.ActionBarButton</item>
    <item name="android:actionMenuTextAppearance">@style/TradeHero.Dashboard.ActionBar.Menu</item>
    <item name="android:actionBarItemBackground">@drawable/basic_transparent_selector</item>
    <item name="android:actionMenuTextColor">@color/tradehero_blue</item>
    <item name="android:actionOverflowButtonStyle">@style/TradeHero.Dashboard.ActionBar.OverflowButtonStyle</item>
    <item name="android:popupMenuStyle">@style/TradeHero.Dashboard.ActionBar.PopupMenu</item>
    <item name="android:actionBarWidgetTheme">@style/TradeHero.Dashboard.ActionBar.WidgetStyle</item>
    <item name="android:dropDownListViewStyle">@style/TradeHero.Dashboard.ActionBar.DropDownListView</item>
    <item name="vpiTabPageIndicatorStyle">@style/Widget.TabPageIndicator</item>
  </style>

  <style name="TradeHero.Authentication.EditText" parent="@android:style/Widget.EditText">
    <item name="android:ems">10</item>
    <item name="android:paddingRight">@dimen/common_margin_4</item>
    <item name="android:paddingTop">@dimen/common_margin_4</item>
    <item name="android:paddingBottom">@dimen/common_margin_4</item>
    <item name="android:paddingLeft">@dimen/common_margin_4</item>
    <item name="android:drawablePadding">@dimen/common_margin_2</item>
    <item name="android:textColor">@color/black</item>
  </style>

  <style name="TradeHero.Dashboard.ActionBar" parent="Widget.AppCompat.Toolbar">
    <item name="android:background">?attr/colorPrimary</item>
    <item name="theme">@style/ThemeOverlay.ActionBar.TradeHero</item>
    <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
    <item name="contentInsetStart">0dp</item>
  </style>

  <style name="ThemeOverlay.ActionBar.TradeHero" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
    <item name="android:windowActionBarOverlay">true</item>
    <item name="windowActionBarOverlay">true</item>
  </style>

  <style name="TradeHero.Dashboard.ActionBarButton" parent="Widget.AppCompat.Light.ActionButton">
    <item name="android:background">@drawable/basic_transparent_selector</item>

  </style>

  <style name="TradeHero.Dashboard.ActionBar.TitleTextStyle"
      parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
    <item name="android:textColor">@color/white</item>
  </style>

  <style name="TradeHero.Dashboard.ActionBar.Menu"
      parent="TextAppearance.AppCompat.Widget.ActionBar.Menu">
    <item name="android:textSize">@dimen/text_medium</item>
    <item name="android:textAllCaps">false</item>
  </style>

  <style name="TradeHero.Dashboard.ActionBar.DropDownListView" parent="Widget.AppCompat.Light.ListView.DropDown">
    <item name="android:listSelector">@drawable/basic_transparent_selector</item>
  </style>

  <style name="TradeHero.Dashboard.ActionBar.PopupMenu" parent="Widget.AppCompat.Light.PopupMenu">
    <item name="android:listSelector">@drawable/basic_transparent_selector</item>
  </style>

  <style name="TradeHero.Dashboard.ActionBar.OverflowButtonStyle" parent="Widget.AppCompat.Light.ActionButton.Overflow">
  </style>

  <style name="TradeHero.Dashboard.ActionBar.WidgetStyle" parent="Widget">
    <item name="android:textColor">@color/tradehero_blue</item>
  </style>

  <style name="TH.ListItem">
    <item name="android:background">@drawable/list_item_bg</item>
    <item name="android:minHeight">48dp</item>
  </style>

  <style name="TutorialTextContent">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textSize">@dimen/text_small</item>
    <item name="android:textColor">@color/white</item>
  </style>

  <style name="TutorialTextHeader">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:gravity">center_vertical</item>
    <item name="android:textSize">@dimen/text_header</item>
    <item name="android:textColor">@color/white</item>
    <item name="android:textStyle">bold</item>
  </style>

  <style name="TH.VoteView" parent="TH.DiscussionActionButtons.Text">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:gravity">center_vertical</item>
  </style>

  <style name="TH.Btn_BuySell_Action">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:textSize">@dimen/text_small</item>
    <item name="android:textColor">@color/tradehero_blue</item>
    <item name="android:singleLine">true</item>
    <item name="android:ellipsize">middle</item>
    <item name="android:layout_marginLeft">5dp</item>
    <item name="android:layout_marginRight">5dp</item>
    <item name="android:gravity">center</item>
    <item name="android:minHeight">@dimen/button_height_30</item>
    <item name="android:background">@drawable/basic_white_selector</item>
  </style>

  <style name="TH.SeekBar" parent="android:Widget.Holo.Light.SeekBar">
  </style>

  <style name="TH.Btn_Invite_Action">
    <item name="android:background">@drawable/leaderboard_user_item_follow_action_button</item>
    <item name="android:textColor">@color/white</item>
    <item name="android:textSize">@dimen/text_medium</item>
    <item name="android:gravity">center</item>
    <item name="android:paddingTop">5dp</item>
    <item name="android:paddingBottom">5dp</item>
    <item name="android:paddingLeft">10dp</item>
    <item name="android:paddingRight">10dp</item>
    <item name="android:minWidth">100dp</item>
  </style>

  <style name="TH.Time_Text">
    <item name="android:textSize">@dimen/text_xsmall</item>
    <item name="android:textColor">#777</item>
  </style>

  <style name="TH.Time_Text.Header">
    <item name="android:textSize">@dimen/text_xlarge</item>
    <item name="android:textColor">@color/white</item>
  </style>

  <style name="TH.Time_Text.Header.Inverse">
    <item name="android:textSize">@dimen/text_xlarge</item>
    <item name="android:textColor">@color/black</item>
  </style>

  <style name="TH.Loading_Error_Text">
    <item name="android:textSize">@dimen/text_medium</item>
    <item name="android:textColor">#777</item>
    <item name="android:textStyle">italic</item>
  </style>

  <style name="TH.Loading_Empty_Text" parent="@style/TH.Loading_Error_Text">
  </style>

  <style name="TH.BuySell_ViewPager_Button">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:paddingTop">5dp</item>
    <item name="android:paddingBottom">5dp</item>
    <item name="android:textColor">@color/btn_twitter_color_end</item>
    <item name="android:gravity">center</item>
    <item name="android:background">@drawable/security_btn_buy</item>
    <item name="android:textSize">@dimen/text_xsmall</item>
  </style>

  <style name="TH.UserNameTitle">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textColor">@color/black</item>
    <item name="android:textSize">@dimen/text_medium</item>
    <item name="android:textStyle">bold</item>
    <item name="android:ellipsize">middle</item>
    <item name="android:singleLine">true</item>
  </style>

  <style name="TH.ShareButtons">
    <item name="android:layout_width">28dp</item>
    <item name="android:layout_height">28dp</item>
    <item name="android:layout_marginRight">2dp</item>
    <item name="android:layout_marginLeft">2dp</item>
    <item name="android:contentDescription">@null</item>
    <item name="android:scaleType">fitCenter</item>
    <item name="android:checked">false</item>
    <item name="android:gravity">center</item>
    <item name="android:textOff">""</item>
    <item name="android:textOn">""</item>
  </style>

  <style name="TH.TextSocialButtons">
    <item name="android:layout_width">28dp</item>
    <item name="android:layout_height">28dp</item>
    <item name="android:layout_marginRight">2dp</item>
    <item name="android:layout_marginLeft">2dp</item>
    <item name="android:checked">false</item>
    <item name="android:gravity">center</item>
  </style>

  <style name="TH.NumericalAnimatedTextView">
    <item name="android:gravity">center</item>
    <item name="animatedDuration">2500</item>
    <item name="animatedInterval">50</item>
    <item name="showAnimation">true</item>
    <item name="textSize">@dimen/text_large</item>
  </style>

  <style name="TH.DiscussionActionButtons">
    <item name="android:paddingLeft">@dimen/discussion_action_buttons_horizontal_padding</item>
    <item name="android:paddingRight">@dimen/discussion_action_buttons_horizontal_padding</item>
    <item name="android:paddingTop">@dimen/discussion_action_buttons_vertical_padding</item>
    <item name="android:paddingBottom">@dimen/discussion_action_buttons_vertical_padding</item>
  </style>

  <style name="TH.DiscussionActionButtons.Text" parent="TH.DiscussionActionButtons">
    <item name="android:textColor">@drawable/timeline_action_button_text_color_xml</item>
    <item name="android:textSize">@dimen/text_medium</item>
    <item name="android:layout_gravity">center_vertical</item>
    <item name="android:drawablePadding">@dimen/discussion_action_buttons_drawable_padding</item>
  </style>

  <style name="TH.DiscussionTextBox">
    <item name="android:gravity">left|center_vertical</item>
    <item name="android:background">@drawable/textbox_light_background</item>
    <item name="android:padding">@dimen/discussion_action_buttons_drawable_padding</item>
    <item name="android:textSize">@dimen/text_small</item>
    <item name="android:inputType">text|textMultiLine|textImeMultiLine|textCapSentences|textAutoCorrect|textAutoComplete</item>
  </style>

  <style name="TH.DiscussionPostButtons">
    <item name="android:gravity">center</item>
    <item name="android:padding">@dimen/discussion_action_buttons_drawable_padding</item>
    <item name="android:textSize">@dimen/text_small</item>
    <item name="android:textStyle">bold</item>
    <item name="android:textColor">@color/tradehero_blue</item>
  </style>


  <style name="MMTabButton">
    <item name="android:textSize">12.0dip</item>
    <item name="android:gravity">bottom</item>
    <!--<item name="android:background">@drawable/bg_checkbox_menus</item>-->
    <item name="android:layout_width">25dp</item>
    <item name="android:layout_height">25dp</item>
    <item name="android:button">@null</item>
    <item name="android:textColor">@color/white</item>
    <item name="android:layout_weight">1.0</item>
    <item name="android:paddingBottom">2.0dip</item>
    <item name="android:paddingTop">2.0dip</item>
  </style>

  <style name="navigation_icon">
    <!--<item name="android:background">@drawable/style_navigation_radio</item>-->
    <item name="android:layout_width">28dp</item>
    <item name="android:layout_height">28dp</item>
    <item name="android:layout_marginTop">2dp</item>
    <item name="android:layout_marginBottom">3dp</item>
    <!--<item name="android:duplicateParentState">true</item>-->
  </style>

  <style name="navigation_text">
    <!--<item name="android:background">@drawable/style_navigation_radio</item>-->
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_marginTop">1dp</item>
    <item name="android:textColor">@color/tab_main_text_selector</item>
    <item name="android:textSize">12dip</item>
    <!--<item name="android:duplicateParentState">true</item>-->
  </style>


  <style name="trade_of_me_title_text">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">28dp</item>
    <item name="android:singleLine">true</item>
    <item name="android:textColor">@color/text_gray_normal</item>
    <item name="android:textSize">16dip</item>
    <item name="android:gravity">bottom</item>
    <item name="android:layout_alignParentRight">true</item>
  </style>

  <style name="trade_of_me_value_text">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">28dp</item>
    <item name="android:singleLine">true</item>
    <item name="android:textColor">@color/text_gray_normal</item>
    <item name="android:textSize">16dip</item>
    <item name="android:gravity">bottom</item>
    <item name="android:layout_alignParentLeft">true</item>
  </style>

  <style name="security_data_info">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textColor">#999</item>
    <item name="android:singleLine">true</item>
  </style>

  <style name="buy_sale_title">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:singleLine">true</item>
    <item name="android:textSize">18sp</item>
    <item name="android:textColor">@color/text_gray_normal</item>
  </style>

  <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
    <item name="android:button">@drawable/checkbox_xml</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:singleLine">true</item>
    <item name="android:textSize">18sp</item>
    <item name="android:textColor">@color/text_gray_normal</item>
  </style>

  <style name="buy_sale_item">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">40dp</item>
    <item name="android:paddingLeft">20dp</item>
    <item name="android:paddingRight">20dp</item>
  </style>

  <style name="competition_create">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textColor">@color/black</item>
    <item name="android:textSize">16sp</item>

  </style>

  <style name="TH.Dialog" parent="Theme.AppCompat">
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:colorBackgroundCacheHint">@null</item>
    <item name="android:windowFrame">@null</item>
    <item name="android:windowContentOverlay">@null</item>
    <item name="android:windowAnimationStyle">@null</item>
    <item name="android:windowIsFloating">true</item>
    <item name="android:backgroundDimEnabled">true</item>
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowCloseOnTouchOutside">true</item>
  </style>

</resources>